项目三：超过5名学生的课
CREATE TABLE courses( %创建表格
student VARCHAR(255) NOT NULL,
class VARCHAR(255) NOT NULL
);

INSERT INTO courses %插入内容
  VALUES('A','Math');
INSERT INTO courses
  VALUES('B','English');
INSERT INTO courses
  VALUES('C','Math');
INSERT INTO courses
  VALUES('D','Biology');
INSERT INTO courses
  VALUES('E','Math');
INSERT INTO courses
  VALUES('F','Computer');
INSERT INTO courses
  VALUES('G','Math');
INSERT INTO courses
  VALUES('H','Math');
INSERT INTO courses
  VALUES('I','Math');
INSERT INTO courses
  VALUES('A','Math');

SELECT class FROM courses %查找内容
GROUP BY class HAVING COUNT(DISTINCT student)>=5;

项目四：交换工资
CREATE TABLE salary( %创建
id INT(255) PRIMARY KEY,
name VARCHAR(255) NOT NULL,
sex VARCHAR(255) NOT NULL,
salary INT(255) NOT NULL
);

INSERT INTO salary %输入
	VALUES('1','A','m','2500');
INSERT INTO salary
	VALUES('2','B','f','1500');
INSERT INTO salary
	VALUES('3','C','m','5500');
INSERT INTO salary
	VALUES('4','D','f','500');
  
SELECT id,name, CASE %交换
	WHEN sex='f' THEN 'm'
    ELSE 'f'
END sex, salary
FROM salary;

